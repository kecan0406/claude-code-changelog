{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/install-success",
      "destination": "/install-success.html"
    },
    {
      "source": "/install-error",
      "destination": "/install-error.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 256,
      "maxDuration": 30
    }
  }
}
